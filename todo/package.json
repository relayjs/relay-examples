{
  "private": true,
  "scripts": {
    "start": "babel-node ./server.js",
    "build": "relay-compiler --src ./js/ --schema ./data/schema.graphql",
    "update-schema": "babel-node ./scripts/updateSchema.js",
    "prettier": "find . \\( -name node_modules -o -name __generated__ \\) -prune -or \\( -name '*.js' -o -name '*.css' -o -name '*.json' -o -name '*.md' \\) -print | xargs prettier --write"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^14.0.2",
    "graphql-relay": "^0.5.5",
    "prettier": "^1.14.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-relay": "^1.7.0",
    "todomvc-app-css": "^2.1.2",
    "todomvc-common": "^1.0.3",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-relay": "^1.7.0",
    "relay-compiler": "^1.7.0",
    "webpack": "^4.18.0",
    "webpack-dev-server": "^3.1.8"
  },
  "prettier": {
    "singleQuote": true,
